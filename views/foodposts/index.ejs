<%- include('../partials/header') %>

<table id="list">
    <thead>
      <tr>
        <th>Name</th>
        <th>Category</th>
        <th>Posts</th>
      </tr>
    </thead>
    <tbody>
      <% foodposts.forEach(function(foodpost) { %>
        <tr>
          <td><%= foodpost.name %></td>
          <td><%= foodpost.category %></td>
          <td><a href="/foodposts/<%= foodpost.id %>">DETAILS</a></td>
          <td>
            <form action="/foodposts/<%= foodpost.id %>?_method=DELETE" method="POST">
              <button type="submit">X</button>
            </form>
          </td>
        </tr>
      <% }); %>
      </tbody>
</table>
 
<%- include('../partials/footer') %>

